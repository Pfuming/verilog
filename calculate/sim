S_0x59c28cc5ab80 .scope module, "tb_arithmetic" "tb_arithmetic" 2 2;
 .timescale -9 -10;
v0x59c28cc6e570_0 .var "a", 7 0;
v0x59c28cc6e650_0 .var "b", 7 0;
v0x59c28cc6e720_0 .net "result", 7 0, v0x59c28cc6e2f0_0;  1 drivers
v0x59c28cc6e820_0 .var "sel", 1 0;
S_0x59c28cc5ad10 .scope module, "u1" "arithmetic" 2 8, 3 1 0, S_0x59c28cc5ab80;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "a";
    .port_info 1 /INPUT 8 "b";
    .port_info 2 /INPUT 2 "sel";
    .port_info 3 /OUTPUT 8 "result";
v0x59c28cc20490_0 .net "a", 7 0, v0x59c28cc6e570_0;  1 drivers
v0x59c28cc6e210_0 .net "b", 7 0, v0x59c28cc6e650_0;  1 drivers
v0x59c28cc6e2f0_0 .var "result", 7 0;
v0x59c28cc6e3e0_0 .net "sel", 1 0, v0x59c28cc6e820_0;  1 drivers
E_0x59c28cc20dc0 .event anyedge, v0x59c28cc6e3e0_0, v0x59c28cc20490_0, v0x59c28cc6e210_0;
    .scope S_0x59c28cc5ad10;
T_0 ;
    %wait E_0x59c28cc20dc0;
    %load/vec4 v0x59c28cc6e3e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 255, 255, 8;
    %store/vec4 v0x59c28cc6e2f0_0, 0, 8;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x59c28cc20490_0;
    %load/vec4 v0x59c28cc6e210_0;
    %add;
    %store/vec4 v0x59c28cc6e2f0_0, 0, 8;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x59c28cc20490_0;
    %load/vec4 v0x59c28cc6e210_0;
    %sub;
    %store/vec4 v0x59c28cc6e2f0_0, 0, 8;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x59c28cc20490_0;
    %load/vec4 v0x59c28cc6e210_0;
    %mul;
    %store/vec4 v0x59c28cc6e2f0_0, 0, 8;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x59c28cc20490_0;
    %load/vec4 v0x59c28cc6e210_0;
    %div;
    %store/vec4 v0x59c28cc6e2f0_0, 0, 8;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x59c28cc5ab80;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x59c28cc5ab80 {0 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x59c28cc6e570_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x59c28cc6e650_0, 0, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x59c28cc6e820_0, 0, 2;
    %delay 200, 0;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x59c28cc6e570_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x59c28cc6e650_0, 0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x59c28cc6e820_0, 0, 2;
    %delay 200, 0;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x59c28cc6e570_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x59c28cc6e650_0, 0, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x59c28cc6e820_0, 0, 2;
